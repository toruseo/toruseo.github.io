
body {
	  
	color: #000000;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	font-size: 100%;
	background: #ffffff;
	text-align: left;
	
	font-size: 16px;
	font-family: "メイリオ",Meiryo,"ヒラギノ角ゴ Pro W3","Hiragino Kaku Gothic Pro",Osaka,"ＭＳ Ｐゴシック","MS PGothic",sans-serif;
	line-height: 1.6;
}

h1 {
	  
	color: #000000;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	font-size: 200%;
	font-weight: bold;
	text-align: center;
}

h2 {
	  
	color: #000000;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	font-size: 150%;
	font-weight: normal;
	text-align: center;
}

h3 {
	background-color: #005396;
	text-indent: 0.6em;  
	border-left: solid 12px #000000;
	color: #ffffff;
	margin-top: 2px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	font-size: 125%;
	font-weight: bold;
}

h4 {
	background-color: #e5edf4;
	text-indent: 0.4em;  
	border-left: solid 4px #000000; 
	color: #005396;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	font-size: 110%;
	font-weight: bold;
}

h5 {
	  
	color: #005396;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 5px;
	font-size: 100%;
	font-weight: bold;
}

h6 {
	  
	color: #005396;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 5px;
	font-size: 100%;
	font-weight: bold;
}


dl.etaremuneList {
    overflow: hidden;
    zoom: 1;
}

dl.etaremuneList dt {
    width: 30px;
    float: left;
    clear: both;
    font-weight: bold;
    text-align:right;
}

dl.etaremuneList dd {
    padding: 0 0 5px 5px;
}
